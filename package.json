{
  "name": "ion2-calendar-dayjs",
  "version": "3.5.0",
  "description": "A date picker component for ionic 6+",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "start": "cd ./dev && npm start",
    "clean:dist": "rm -rf dist",
    "demo:link": "npm link && cd ./demo && npm link ion2-calendar",
    "demo:serve": "cd ./demo && ionic serve",
    "build": "run-s clean:dist build:copy-sources build:compile-scss build:ts",
    "build:ts": "tsc && ngc",
    "build:copy-sources": "gulp copy-sources",
    "build:compile-scss": "gulp copy-scss && gulp rename-scss && gulp clean-scss",
    "test": "echo \"Error: no test specified\"",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "packagr": "ng-packagr -p package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/spasma/ion2-calendar.git"
  },
  "devDependencies": {
    "@angular/common": "14.2.0",
    "@angular/compiler": "14.2.0",
    "@angular/compiler-cli": "^14.2.0",
    "@angular/core": "14.2.0",
    "@angular/forms": "14.2.0",
    "@angular/platform-browser": "14.2.0",
    "@angular/platform-browser-dynamic": "14.2.0",
    "@angular/router": "14.2.0",
    "@ionic/angular": "6.2.5",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-clean": "^0.4.0",
    "gulp-ext-replace": "^0.3.0",
    "gulp-sass": "^5.1.0",
    "ionicons": "6.0.3",
    "dayjs": "^1.11.5",
    "ng-packagr": "^14.2.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "7.5.6",
    "sass": "^1.54.8",
    "tsickle": "^0.46.3",
    "tslint": "~5.20.1",
    "typescript": "~4.7.2",
    "zone.js": "0.11.8"
  },
  "keywords": [
    "ionic2",
    "calendar",
    "Datepicker",
    "date-range"
  ],
  "author": "HsuanLee",
  "license": "MIT",
  "peerDependencies": {
    "moment": "^2.19.3"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/index.ts",
      "styleIncludePaths": [
        ""
      ],
      "umdModuleIds": {
        "moment": "moment"
      }
    }
  }
}
